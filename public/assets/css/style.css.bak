.wrap {
  margin: auto;
  display: flex;
  width: calc(var(--size) * 5);
  align-items: center;
  justify-content: flex-start;
}
.wrap .numbers {
  display: flex;
  align-items: center;
  justify-content: center;
}
.wrap .numbers .minus,
.wrap .numbers .plus {
  font-size: var(--fontSize);
  color: var(--white);
  position: relative;
  -webkit-user-select: none;
     -moz-user-select: none;
      -ms-user-select: none;
          user-select: none;
}
.wrap .numbers .minus:active,
.wrap .numbers .plus:active {
  color: var(--aqua);
}
.wrap .numbers .minus.gray,
.wrap .numbers .plus.gray {
  color: var(--dark);
}
.wrap .numbers .minus.gray::after,
.wrap .numbers .plus.gray::after {
  background-color: var(--dark) !important;
}
.wrap .numbers .minus {
  width: 2rem;
}
.wrap .numbers .minus:active::after {
  background-color: var(--aqua);
}
.wrap .numbers .minus::after {
  content: "";
  position: absolute;
  top: 50%;
  right: 25%;
  height: 90%;
  height: calc(var(--size) * 0.05);
  width: calc(var(--size) * 0.2);
  background-color: #fff;
}
.wrap .numbers .box {
  margin: 0 0.5rem;
  background-color: var(--dark);
  box-shadow: inset 0 0.125rem 0.25rem var(--darker);
  border-radius: calc(var(--size) / 7.5);
  overflow: hidden;
  width: var(--size);
  height: var(--size);
  display: flex;
  align-items: center;
  justify-content: flex-start;
  flex-wrap: no-wrap;
  scroll-behavior: smooth;
}
.wrap .numbers .box .number {
  min-width: 100%;
  text-align: center;
  color: var(--white);
  font-size: var(--fontSize);
}
.wrap .button {
  padding: 0 1rem;
  margin-left: 2rem;
  display: flex;
  align-items: center;
  justify-content: center;
  height: var(--size);
  background-color: var(--aqua);
  box-shadow: 0 0 calc(var(--size) / 6) var(--cart);
  border-radius: calc(var(--size) / 7.5);
}
.wrap .button .cart {
  padding: 0 1.5rem;
  width: var(--fontSize);
  height: var(--fontSize);
  background-color: var(--cart);
  -webkit-mask: var(--cartSrc) no-repeat center;
          mask: var(--cartSrc) no-repeat center;
  box-shadow: inset 0 0 1.5rem #0002;
  transform-origin: center;
  transform: scale(0.75);
}
.wrap .button .price {
  font-size: var(--fontSize);
  color: var(--black);
}

