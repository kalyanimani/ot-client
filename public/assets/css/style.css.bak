.zoom-card {
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center;
  height: 100%;
  transition: all 0.5s;
  width: 100%;
}
.zoom-card .card__link-wrapper {
  display: block;
  height: 100%;
  width: 100%;
}
.zoom-card .card__link-wrapper .content__inner {
    color: #ffffff;
    font-size: 24px;
    left: 50%;
    position: absolute;
    top: 75%;
    transform: translate(-50%, -50%);
    z-index: 1;
}
.content__inner i {
    color: #ffffff;
    left: 0%;
    position: relative;
    top: 60px;
    width: 100%;
    font-weight: 300;
    font-size: 30px;
    display: inline-block;
}
.zoom-card .card__link-wrapper::after {
  background-color: rgba(0, 0, 0, 0.4);
  content: "";
  display: block;
  height: 100%;
  left: 0;
  position: absolute;
  top: 0;
  width: 100%;
}

.gradient {
  background-image: linear-gradient(180deg, rgba(174, 145, 117, 0) 30%, #ae9276);
  height: 100%;
  left: 0;
  opacity: 0;
  position: absolute;
  pointer-events: none;
  transition: 0.4s cubic-bezier(0.455, 0.03, 0.515, 0.955);
  top: 0;
  width: 100%;
  z-index: 1;
}
